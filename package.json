{
  "name": "back-end",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "find . -name '*.test.ts' -delete && rm -rf dist",
    "build": "npm run clean && tsc --noEmitOnError",
    "start": "npm run build && cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint . --ext .ts --parser-options project=./tsconfig.json",
    "lint:fix": "eslint . --ext .ts --fix --parser-options project=./tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.1",
    "nodemon": "^3.1.9"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "typescript": "^5.8.2"
  }
}
